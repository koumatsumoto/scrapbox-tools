{
  "name": "scrapbox-tools",
  "version": "0.3.1",
  "repository": "git@github.com:kouMatsumoto/scrapbox-tools.git",
  "author": "Kou Matsumoto <developer@kou-matsumoto.jp>",
  "license": "MIT",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/es/index.js",
  "bin": {
    "scrapbox-tools": "./dist/cli/bin.js"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "rollup --config",
    "check": "concurrently 'tsc --noEmit' 'npm run test' 'npm run check:lint' 'npm run build'",
    "check:lint": "npm run format && git diff --exit-code --name-only .",
    "format": "eslint 'src/**/*.ts' --fix && prettier --write .",
    "prepack": "npm ci && npm run build",
    "test": "jest -c jest.config.js --ci --silent --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "commander": "^8.1.0",
    "date-fns": "^2.21.3",
    "inquirer": "^8.1.2",
    "node-fetch": "^2.6.1",
    "ws": "^7.4.5"
  },
  "peerDependencies": {
    "rxjs": "^7.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/inquirer": "^7.3.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.3",
    "@types/ws": "^7.4.4",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "concurrently": "^6.1.0",
    "dotenv": "^9.0.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "rollup": "^2.56.1",
    "rollup-plugin-add-shebang": "^0.3.1",
    "rxjs": "^7.3.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typescript": "^4.3.5"
  }
}
