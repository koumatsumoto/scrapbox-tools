{
  "name": "scrapbox-tools",
  "version": "0.3.1",
  "repository": "git@github.com:kouMatsumoto/scrapbox-tools.git",
  "author": "Kou Matsumoto <developer@kou-matsumoto.jp>",
  "license": "MIT",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/es/index.js",
  "bin": {
    "scrapbox-tools": "./dist/cli/bin.js"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "rollup --config",
    "check": "concurrently 'tsc --noEmit' 'npm run test' 'npm run check:lint' 'npm run build'",
    "check:lint": "npm run format && git diff --exit-code --name-only .",
    "format": "eslint 'src/**/*.ts' --fix && prettier --write .",
    "prepack": "npm ci && npm run build",
    "test": "jest -c jest.config.js --ci --silent --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "commander": "^8.1.0",
    "date-fns": "^2.23.0",
    "inquirer": "^8.1.2",
    "node-fetch": "^2.6.1",
    "ws": "^8.2.0"
  },
  "peerDependencies": {
    "rxjs": "^7.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/inquirer": "^7.3.3",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.2",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "concurrently": "^6.2.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.1",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "rollup": "^2.56.2",
    "rollup-plugin-add-shebang": "^0.3.1",
    "rxjs": "^7.3.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  }
}
